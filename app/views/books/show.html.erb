<%= link_to "Home", books_path %>

<h1>Books#show</h1>

<table>
  <tbody>
    <tr>
      <td>Author</td>
      <td><%= @book.author %></td>
    </tr>
    <tr>
      <td>Title</td>
      <td><%= @book.title %></td>
    </tr>
    <tr>
      <td>Genre</td>
      <td><%= @book.genre %></td>
    </tr>
    <tr>
      <td>Price</td>
      <td><%= @book.price %></td>
    </tr>
    <tr>
      <td>Published Date</td>
      <td><%= @book.published_date %></td>
    </tr>
  </tbody>
</table>